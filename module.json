{
  "id": "pf2e-dorako-ui",
  "title": "PF2e Dorako UI",
  "description": "Overhauls the Foundry UI and several modules.",
  "url": "https://github.com/Dorako/pf2e-dorako-ui",
  "authors": [
    {
      "name": "Dorako",
